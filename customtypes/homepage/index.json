{
  "format": "page",
  "id": "homepage",
  "label": "Home Page",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "headline": {
        "type": "Text",
        "config": {
          "label": "Headline",
          "placeholder": ""
        }
      },
      "intro": {
        "type": "StructuredText",
        "config": {
          "label": "Intro",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink,embed,list-item,o-list-item"
        }
      },
      "picture": {
        "type": "Image",
        "config": {
          "label": "Picture",
          "constraint": {
            "width": 540
          },
          "thumbnails": []
        }
      },
      "key_skills": {
        "type": "Group",
        "config": {
          "label": "Key Skills",
          "repeat": true,
          "fields": {
            "tech": {
              "type": "Link",
              "config": {
                "label": "Tech",
                "select": "document",
                "customtypes": ["tech"]
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      },
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      }
    }
  }
}

{
  "format": "custom",
  "id": "job",
  "label": "Job",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "employer": {
        "type": "Text",
        "config": {
          "label": "Employer",
          "placeholder": ""
        }
      },
      "employer_logo": {
        "type": "Image",
        "config": {
          "label": "Employer Logo",
          "constraint": {
            "height": null,
            "width": 120
          },
          "thumbnails": []
        }
      },
      "employer_website": {
        "type": "Link",
        "config": {
          "label": "Employer Website",
          "placeholder": "",
          "select": null
        }
      },
      "job_title": {
        "type": "Text",
        "config": {
          "label": "Job Title",
          "placeholder": ""
        }
      },
      "type": {
        "type": "Select",
        "config": {
          "label": "Type",
          "placeholder": "",
          "options": [
            "Permanent",
            "Contract",
            "Spare Time Freelance",
            "Volunteering"
          ]
        }
      },
      "date_start": {
        "type": "Date",
        "config": {
          "label": "Date Start",
          "placeholder": ""
        }
      },
      "date_end": {
        "type": "Date",
        "config": {
          "label": "Date End",
          "placeholder": ""
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "tech": {
        "type": "Group",
        "config": {
          "label": "Tech",
          "repeat": true,
          "fields": {
            "tech": {
              "type": "Link",
              "config": {
                "label": "Tech",
                "select": "document",
                "customtypes": ["tech"]
              }
            }
          }
        }
      },
      "projects": {
        "type": "Group",
        "config": {
          "label": "Projects",
          "repeat": true,
          "fields": {
            "project": {
              "type": "Link",
              "config": {
                "label": "Project",
                "select": "document",
                "customtypes": ["project"]
              }
            }
          }
        }
      }
    }
  }
}
